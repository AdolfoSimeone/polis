<div class="analyze-group-member-container">

{{#each ptptois}}
  <div class="analyze-group-member">
    <img src="{{picture}}">
    {{#if name}}
      &nbsp;
      <p class="analyze-group-member-name">{{name}}</p>
    {{/if}}
    {{#if location}}
      &nbsp;&bull;&nbsp;
      <p class="analyze-group-member-city">{{location}}</p>
    {{/if}}
    {{#if hasSocial}}
      &nbsp;&bull;&nbsp;
      {{#if twitter_url}}
        <a href="{{twitter_url}}" target="_blank"><i class="fa fa-twitter"></i></a> &nbsp;
      {{/if}}
      {{#if facebook_url}}
        <a href="{{facebook_url}}" target="_blank"><i class="fa fa-facebook"></i></a> &nbsp;
      {{/if}}
    {{/if}}
  </div>
{{/each}}


{{#if ptptoisLength}}
  {{#if othersCount}}
    <div class="analyze-group-member-others">
      <p>+{{othersCount}} Others</p>
    </div>
  {{/if}}
{{/if}}

</div>



